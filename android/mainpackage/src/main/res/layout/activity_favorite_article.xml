<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".favorite_article">

	<TextView
		android:id="@+id/txtFavoriteArticleTitle"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="128px"
		android:layout_marginTop="94px"
		android:fontFamily="@font/segoe_ui_semibold"
		android:text="Your Article"
		android:textColor="#565656"
		android:textSize="101px"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageButton
		android:id="@+id/imgbtnFavoriteArticleBack"
		android:layout_width="66px"
		android:layout_height="66px"
		android:layout_marginStart="28px"
		android:layout_marginTop="128px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/back" />
</androidx.constraintlayout.widget.ConstraintLayout>