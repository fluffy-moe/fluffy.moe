<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="294dp"
	android:layout_height="256dp"
	android:background="@drawable/ic_rectangle_93">

	<TextView
		android:id="@+id/txtIdentificationTitle"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="31dp"
		android:layout_marginTop="32dp"
		android:fontFamily="@font/segoe_ui_semibold"
		android:text="@string/title_title"
		android:textColor="#9497A7"
		android:textSize="12dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_begin="32dp" />

	<TextView
		android:id="@+id/txtIdentificationDate"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="80dp"
		android:fontFamily="@font/segoe_ui_semibold"
		android:text="@string/text_date"
		android:textColor="#9497A7"
		android:textSize="12dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/txtIdentificationNote"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="128dp"
		android:fontFamily="@font/segoe_ui_semibold"
		android:text="@string/text_note"
		android:textColor="#9497A7"
		android:textSize="12dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/etIdentificationTitle"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="51dp"
		android:background="@drawable/blank"
		android:ems="10"
		android:fontFamily="@font/segoe_ui_semibold"
		android:inputType="textPersonName"
		android:paddingStart="10dp"
		android:text="@string/text_food_title"
		android:textColor="#E9DBBA"
		android:textSize="8dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/etIdentificationDate"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="98dp"
		android:background="@drawable/blank"
		android:ems="10"
		android:fontFamily="@font/segoe_ui_semibold"
		android:inputType="textPersonName"
		android:paddingStart="10dp"
		android:text="@string/mm_dd_yyyy"
		android:textColor="#E9DBBA"
		android:textSize="8dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/etIdentificationNote"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="146dp"
		android:background="@drawable/rectangle"
		android:ems="10"
		android:fontFamily="@font/segoe_ui_semibold"
		android:inputType="textMultiLine"
		android:paddingStart="10dp"
		android:text="@string/hint_for_note"
		android:textColor="#E9DBBA"
		android:textSize="8dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageButton
		android:id="@+id/imgbtnIdentificationConfirm"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="106dp"
		android:layout_marginTop="215dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/confirm" />

	<ImageButton
		android:id="@+id/imgbtnIdentificationOut"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="270dp"
		android:layout_marginTop="9dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/out" />

	<ImageButton
		android:id="@+id/imgbtnIdentificatinPic"
		android:layout_width="83dp"
		android:layout_height="83dp"
		android:layout_marginStart="181dp"
		android:layout_marginTop="51dp"
		android:background="@null"
		android:scaleType="fitCenter"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/pic" />

	<Space
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>