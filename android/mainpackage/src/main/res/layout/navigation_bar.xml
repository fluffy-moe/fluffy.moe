<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="51dp"
	android:orientation="vertical">

	<ImageView
		android:id="@+id/imgNavigateBackgroundPage"
		android:layout_width="360dp"
		android:layout_height="51dp"
		android:layout_marginStart="0dp"
		android:layout_marginTop="0dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/navigate" />

	<ImageButton
		android:id="@+id/imgbtnCameraPage"
		android:layout_width="32dp"
		android:layout_height="32dp"
		android:layout_marginStart="35dp"
		android:layout_marginTop="10dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/camera_gray" />

	<ImageButton
		android:id="@+id/imgbtnMedicalPage"
		android:layout_width="27dp"
		android:layout_height="27dp"
		android:layout_marginStart="103dp"
		android:layout_marginTop="11dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/medical_gray" />

	<ImageButton
		android:id="@+id/imgbtnCalendarPage"
		android:layout_width="27dp"
		android:layout_height="27dp"
		android:layout_marginStart="166dp"
		android:layout_marginTop="11dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/calendar_gray" />

	<ImageButton
		android:id="@+id/imgbtnArticlePage"
		android:layout_width="32dp"
		android:layout_height="32dp"
		android:layout_marginStart="230dp"
		android:layout_marginTop="12dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/book_gray" />

	<ImageButton
		android:id="@+id/imgbtnUserPage"
		android:layout_width="30dp"
		android:layout_height="30dp"
		android:layout_marginStart="299dp"
		android:layout_marginTop="10dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/home_gray" />

</androidx.constraintlayout.widget.ConstraintLayout>