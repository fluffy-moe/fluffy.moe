<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="538.92px"
	android:layout_height="600px"
	android:background="@drawable/ic_rectangle_177"
	tools:context=".Event">

	<ImageButton
		android:id="@+id/imgbtnEventEvent"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="202px"
		android:layout_marginTop="60px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/Event" />

	<TextView
		android:id="@+id/txtEventEvent"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="195px"
		android:layout_marginTop="222px"
		android:fontFamily="@font/segoe_ui_bold"
		android:text="Event"
		android:textColor="#FFFFFF"
		android:textSize="60px"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/txtEventRecord"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="92px"
		android:layout_marginTop="330px"
		android:fontFamily="@font/segoe_ui_semibold"
		android:text="  take a walk\nbuy some food"
		android:textColor="#565656"
		android:textSize="52px"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>