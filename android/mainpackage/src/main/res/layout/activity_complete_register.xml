<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0EFF5"
    tools:ignore="RtlSymmetry" >

    <ImageView
        android:id="@+id/imgBackgroundPage1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/grayer" />

    <ImageView
        android:id="@+id/imgPetBasicsDialogMain4"
        android:layout_width="300dp"
        android:layout_height="545dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="60dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/register_petbasics_dialog" />

    <TextView
        android:id="@+id/txt_breed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="286dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetBreed"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="355dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetGender"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_petbasics2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="62dp"
        android:fontFamily="@font/segoe_ui_bold"
        android:text="@string/title_pet_basics"
        android:textAlignment="center"
        android:textColor="#565656"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_give"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="135dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/introduction_petbasics"
        android:textColor="#565656"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="227dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetName"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_birthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="191dp"
        android:layout_marginTop="285dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetBirthday"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_spayed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="191dp"
        android:layout_marginTop="354dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetSpayed"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/txt_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="421dp"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="@string/dpOptionsPetWeight"
        android:textColor="#565656"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_name"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="247dp"
        android:background="@drawable/round_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="10dp"
        android:text="@string/pets_name"
        android:textColor="#969696"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_breed"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="306dp"
        android:background="@drawable/round_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="10dp"
        android:text="@string/pets_breed"
        android:textColor="#969696"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_birthday"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginStart="189dp"
        android:layout_marginTop="306dp"
        android:background="@drawable/round_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingStart="10dp"
        android:text="@string/mm_dd_yyyy"
        android:textColor="#969696"
        android:textSize="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--<ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160px"
        android:layout_marginTop="1760px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kg" />-->

    <RadioGroup
        android:id="@+id/rbg_weight"
        android:layout_width="280dp"
        android:layout_height="25dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="441dp"
        android:background="@drawable/round_edittext"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/rb_10kg"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:background="@drawable/radio_button_checked"
            android:button="@null"
            android:text="@string/text_0_10kg"
            android:textAlignment="center"
            android:textColor="#565656" />

        <RadioButton
            android:id="@+id/rb_20kg"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:button="@null"
            android:text="@string/text_10_20kg"
            android:textAlignment="center"
            android:textColor="#565656" />

        <RadioButton
            android:id="@+id/rb_30kg"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:button="@null"
            android:text="@string/text_20_30kg"
            android:textAlignment="center"
            android:textColor="#565656" />

        <RadioButton
            android:id="@+id/rb_40kg"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:button="@null"
            android:text="@string/text_30_40kg"
            android:textAlignment="center"
            android:textColor="#565656" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/rbg_yes_no"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginStart="189dp"
        android:layout_marginTop="375dp"
        android:background="@drawable/round_edittext"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/rb_yes"
            android:layout_width="66dp"
            android:layout_height="25dp"
            android:button="@null"
            android:text="@string/text_yes"
            android:textAlignment="center"
            android:textColor="#969696"
            android:textSize="11dp" />

        <RadioButton
            android:id="@+id/rb_no"
            android:layout_width="66dp"
            android:layout_height="25dp"
            android:background="@drawable/radio_button_checked"
            android:button="@null"
            android:text="@string/text_no"
            android:textAlignment="center"
            android:textColor="#969696"
            android:textSize="11dp" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/rbg_gender"
        android:layout_width="132dp"
        android:layout_height="25dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="375dp"
        android:background="@drawable/round_radio_button_group"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/rb_female"
            android:layout_width="66dp"
            android:layout_height="25dp"
            android:button="@null"
            android:text="@string/text_female"
            android:textAlignment="center"
            android:textColor="#969696"
            android:textSize="11dp" />

        <RadioButton
            android:id="@+id/rb_male"
            android:layout_width="66dp"
            android:layout_height="25dp"
            android:background="@drawable/radio_button_checked"
            android:button="@null"
            android:text="@string/text_male"
            android:textAlignment="center"
            android:textColor="#969696"
            android:textSize="11dp" />
    </RadioGroup>

    <ImageButton
        android:id="@+id/imgbtnRegister2Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="548dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_register" />

    <ImageButton
        android:id="@+id/imgbtnRegister2Next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="243dp"
        android:layout_marginTop="548dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/next" />

</androidx.constraintlayout.widget.ConstraintLayout>