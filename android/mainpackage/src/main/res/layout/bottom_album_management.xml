<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="360dp"
	android:layout_height="216dp">

	<ImageButton
		android:id="@+id/imgbtnRenameAlbum"
		android:layout_width="wrap_content"
		android:layout_height="18dp"
		android:layout_marginStart="52dp"
		android:layout_marginTop="55dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/rename_album" />

	<ImageButton
		android:id="@+id/imgbtnSelectPhoto"
		android:layout_width="wrap_content"
		android:layout_height="18dp"
		android:layout_marginStart="52dp"
		android:layout_marginTop="99dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/select_photo" />

	<ImageButton
		android:id="@+id/imgbtnDeleteAlbum"
		android:layout_width="wrap_content"
		android:layout_height="18dp"
		android:layout_marginStart="52dp"
		android:layout_marginTop="143dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/delete_album" />

	<ImageView
		android:layout_width="223dp"
		android:layout_height="0dp"
		android:layout_marginStart="84dp"
		android:layout_marginTop="86dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profileline" />

	<ImageView
		android:layout_width="223dp"
		android:layout_height="0dp"
		android:layout_marginStart="84dp"
		android:layout_marginTop="130dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profileline" />

	<ImageView
		android:layout_width="71dp"
		android:layout_height="0dp"
		android:layout_marginStart="144dp"
		android:layout_marginTop="8dp"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profilethickline" />

	<View
		android:layout_width="46dp"
		android:layout_height="9dp"
		android:layout_marginStart="220dp"
		android:layout_marginTop="204dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>