<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/imglocationBackground"
	android:layout_width="1135px"
	android:layout_height="345px"
	android:background="@drawable/location_type_background">

	<TextView
		android:id="@+id/txtSearchResultTitle"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="77px"
		android:layout_marginTop="51px"
		android:layout_marginEnd="32dp"
		android:textSize="55px"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/txtClinicName"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="77px"
		android:layout_marginTop="156px"
		android:textColor="#807878"
		android:textSize="40px"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/txtClinicPhone"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="77px"
		android:layout_marginTop="242px"
		android:textColor="#807878"
		android:textSize="40px"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
