<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5">

    <ImageView
        android:id="@+id/imgMedicalTittleBackground"
        android:layout_width="1440px"
        android:layout_height="598px"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_imgmedicaltittlebackground" />

    <TextView
        android:id="@+id/txtMedicalPetWeight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1145px"
        android:layout_marginTop="333px"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="4.5kg"
        android:textColor="#565656"
        android:textSize="59px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtMedicalTittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="86px"
        android:layout_marginTop="86.5px"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="Medical Examination"
        android:textColor="#595959"
        android:textSize="101px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtMedicalAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="826px"
        android:layout_marginTop="412px"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="Age"
        android:textColor="#969696"
        android:textSize="58px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtMedicalWeight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1125px"
        android:layout_marginTop="413px"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="Weight"
        android:textColor="#969696"
        android:textSize="58px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgbtnMedicalClinicSearchIcon"
