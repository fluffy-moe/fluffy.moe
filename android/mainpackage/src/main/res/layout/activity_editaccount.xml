<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="1440px"
	android:layout_height="864px">

	<ImageButton
		android:id="@+id/imgbtnEditAccount"
		android:layout_width="504px"
		android:layout_height="73px"
		android:layout_marginStart="210px"
		android:layout_marginTop="220px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/editaccount" />

	<ImageButton
		android:id="@+id/imgbtnYourArticle"
		android:layout_width="496px"
		android:layout_height="73px"
		android:layout_marginStart="210px"
		android:layout_marginTop="396px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/yourarticle" />

	<ImageButton
		android:id="@+id/imgbtnLogOut"
		android:layout_width="361px"
		android:layout_height="73px"
		android:layout_marginStart="210px"
		android:layout_marginTop="572px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/logout" />

	<ImageView
		android:id="@+id/imgEditAccountLine"
		android:layout_width="893px"
		android:layout_height="0px"
		android:layout_marginStart="338px"
		android:layout_marginTop="345px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profileline" />

	<ImageView
		android:id="@+id/imgEditAccountLineLine"
		android:layout_width="893px"
		android:layout_height="0px"
		android:layout_marginStart="338px"
		android:layout_marginTop="521px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profileline" />

	<ImageView
		android:id="@+id/imgEditAccountThickLine"
		android:layout_width="286px"
		android:layout_height="0px"
		android:layout_marginStart="578px"
		android:layout_marginTop="35px"
		android:background="@null"
		android:scaleType="fitXY"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/profilethickline" />
</androidx.constraintlayout.widget.ConstraintLayout>