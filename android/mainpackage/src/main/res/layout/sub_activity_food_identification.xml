<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="2354px"
    tools:ignore="PxUsage" >

    <TextView
        android:id="@+id/txtFoodTittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128px"
        android:layout_marginTop="94px"
        android:fontFamily="@font/segoe_ui_semibold"
        android:text="Food identification"
        android:textColor="#595959"
        android:textSize="101px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/lvFoods"
        android:layout_width="1207px"
        android:layout_height="wrap_content"
        android:layout_marginStart="117px"
        android:layout_marginTop="287px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
		android:divider="@drawable/divider_61px"
		android:dividerHeight="59px" />
</androidx.constraintlayout.widget.ConstraintLayout>